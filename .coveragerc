[run]
branch = True
omit =
    tests/*
    *__init__*

[report]
exclude_lines =
    # Have to re-enable the standard pragma
    pragma: no cover

    # Don't complain if tests don't hit defensive assertion code:
    raise NotImplementedError
    pass
    # Ignore main code
    if __name__ == .__main__.:

[html]
directory = coverage_report